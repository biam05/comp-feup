/*
Method overloading --> different return type and different parameters type
*/

import io;

class Test1{
    int a;
    int[] b;
    int c;

    public int sum(int a, int[] b, int c){
        return a + b[0] + c;
    }
    public int sum(int a, int b){
        return a + b;
    }
    public boolean sum(int a, int b, int c){
        return a < b &&  b<c;
    }

    public int[] getArray(){
        return b;
    }

    public static void main(String[] args){
        Test1 d;
        int e;
        int g;

        a = 2;
        b = new int[2];
        b[0] = 1;
        c = 5;
        g = 3;

        d = new Test1();
        if(d.sum(a,g,c)){
            e = d.sum(a,b,c);
        }
        else{
           e = d.getArray().length;
        }
        io.println(e);

    }
}