/*
Method overloading
*/

import io;

class Test1{
    int a;
    int b;
    int c;

    public int sum(int a, int b, int c){
        return a + b + c;
    }
    public int sum(int a, int b){
        return a + b;
    }


    public static void main(String[] args){
        int d;
        int e;
        a = 2;
        b = 4;
        c = 5;

        d = this.sum(a,b);
        e = this.sum(a,b,c);
        io.println(d);
        io.println(e);

    }
}